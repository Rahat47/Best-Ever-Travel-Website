    "main": "index.js",
    "scripts": {
        "watch": "node-sass sass/main.scss css/style.css -w",
        "start": "live-server",
        "dev": "npm-run-all --parallel start watch ",
        "compile": "node-sass sass/main.scss css/style.comp.css ",
        "concat": "concat -o css/styles.concat.css css/icon-font.css css/style.comp.css",
        "prefix": "postcss --use autoprefixer -b 'last 10 versions' css/styles.concat.css -o css/styles.prefix.css",
        "compress": "node-sass css/styles.prefix.css css/style.css --output-style compressed",
        "build": "npm-run-all compile concat prefix compress"
